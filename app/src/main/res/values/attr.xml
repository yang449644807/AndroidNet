<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="TitleBarStyle" format="reference" />

    <declare-styleable name="TitleBarView">
        <attr name="bg_color" format="color"></attr>
        <attr name="left_text" format="string" />
        <attr name="title_text_color" format="color"></attr>
        <attr name="left_text_visible" format="enum">
            <enum name="gone" value="8" />
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
        </attr>
        <attr name="left_img_src" format="reference" />
        <attr name="left_img_visible" format="enum">
            <enum name="gone" value="8" />
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
        </attr>
        <attr name="title_text" format="string" />
        <attr name="right_text" format="string" />
        <attr name="right_text_visible" format="enum">
            <enum name="gone" value="8" />
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
        </attr>
        <attr name="right_img_src" format="reference" />
        <attr name="right_img_visible" format="enum">
            <enum name="gone" value="8" />
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
        </attr>
    </declare-styleable>
</resources>
